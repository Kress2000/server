    "only-covered": "bin/only-covered.js",
    "check-total": "bin/check-total.js",
    "update-badge": "bin/update-badge.js",
    "set-gh-status": "bin/set-gh-status.js"
  },
  "files": [
    "bin",
    "src",
    "!src/test/"
  ],
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
